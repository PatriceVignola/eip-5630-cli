{
  "name": "eip-5630-cli",
  "version": "1.0.0",
  "description": "A small CLI application that shows how to encrypt and decrypt files using the functions defined in EIP-5630",
  "type": "module",
  "main": "index.js",
  "author": "Patrice Vignola",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write .",
    "compile": "npx tsc",
    "start:cli": "node dist/cli.js",
    "start": "npm-run-all compile \"start:cli -- {@}\" --"
  },
  "dependencies": {
    "eciesjs": "0.3.15",
    "eip-5630": "1.1.0",
    "keccak256": "1.0.6",
    "readline-sync": "1.4.10",
    "secp256k1": "4.0.3"
  },
  "devDependencies": {
    "@types/expect": "24.3.0",
    "@types/mocha": "10.0.0",
    "@types/node": "18.8.1",
    "@types/readline-sync": "1.4.4",
    "@types/secp256k1": "4.0.3",
    "chai": "4.3.6",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "ts-mocha": "10.0.0",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  }
}
